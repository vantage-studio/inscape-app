{
  "version": 2,
  "routes": [
    {
      "src": "/mockServiceWorker.js",
      "dest": "/mockServiceWorker.js",
      "headers": {
        "Service-Worker-Allowed": "/",
        "Content-Type": "application/javascript"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
